<% if current_user.role == "admin" %>
<%= link_to "Property Verification", admins_path %>
  <h1>Admin's Dashboard</h1>
    <h3>Roles</h3>
    <ul>
      <% @admins.each do |a| %>
        <li>
          <%= a.first_name %> - <strong><%= a.role %></strong>
        </li>
      <% end %>
    </ul>
    <%= render "posts/post", post: @posts %>        
<% else %>
  <h1>Yor are Not Admin</h1>
  <h4>You can't aceess this Page</h4>
<% end %>
